public class Main {
    public static void main(String[] args) {
        int num1 = 5;
        int num2 = 5;

        if (areEqual(num1, num2)) {
            System.out.println(num1 + " и " + num2 + " са равни.");
        } else {
            System.out.println(num1 + " и " + num2 + " не са равни.");
        }
    }

    public static boolean areEqual(int num1, int num2) {
        return num1 == num2;
    }
}
